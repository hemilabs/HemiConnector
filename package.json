{
  "name": "hemiconnector",
  "version": "1.0.0",
  "description": "Application to connect Hemi Network to External Services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest watch",
    "build": "tsc -p ./tsconfig.build.json",
    "collect-metrics": "node --experimental-specifier-resolution=node dist/presentation/cli/CollectMetrics.js",
    "give-points": "bun ./src/presentation/cli/GivePoints.ts"
  },
  "author": "Thomas Alvarenga (thomas@bloq.com)",
  "license": "MIT",
  "dependencies": {
    "hemi-viem": "^1.1.0",
    "typescript": "^5.6.3",
    "uuid": "^11.0.3",
    "viem": "^2.17.3",
    "web3": "^4.14.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitest/coverage-c8": "^0.23.1",
    "bun": "^1.1.34",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.7.1",
    "vitest": "^0.23.1"
  }
}
